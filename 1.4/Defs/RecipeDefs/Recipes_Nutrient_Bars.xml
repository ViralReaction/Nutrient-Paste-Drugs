<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>Make_Nutrient_Bar</defName>
		<label>make ten nutrient bars</label>
		<description>Cut a nutrient block into nutrient bars.</description>
		<jobString>Making nutrient bars.</jobString>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Drug</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>100</workAmount>
		<targetCountAdjustment>1</targetCountAdjustment>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
			<li>DrugLab</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Nutrient_Block</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Nutrient_Block</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Nutrient_Bar>10</Nutrient_Bar>
		</products>
		<researchPrerequisites>
			<li>NutrientPaste</li>
		</researchPrerequisites>
	</RecipeDef>

